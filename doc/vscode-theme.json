{
    "git.confirmSync": false,
    "files.autoSave": "onWindowChange",
    "editor.fontLigatures": true,
    "window.autoDetectHighContrast": false,
    "editor.guides.bracketPairs": false,

    "rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.inlayHints.genericParameterHints.const.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,

    "workbench.colorCustomizations": {

        "[Default Dark Modern]": {
            "editor.background": "#000",
            "editor.lineHighlightBackground": "#123",
            "editorCursor.foreground": "#fff",
            "activityBar.background": "#222",
            "sideBar.background": "#111",
            "statusBar.background": "#08324e",
            "titleBar.activeBackground": "#000",
            "terminal.background":"#000", 
            "editorBracketHighlight.foreground1": "#aaaaaa",
            "editorBracketHighlight.foreground2": "#bbbb99",
            "editorBracketHighlight.foreground3": "#cccc88",
            "editorBracketHighlight.foreground4": "#dddd77",
            "editorBracketHighlight.foreground5": "#eeee66",
            "editorBracketHighlight.foreground6": "#ffff55"
        }
    },
    "editor.tokenColorCustomizations": {
        "[Default Dark Modern]": {
            "comments": "#334444",
            "variables": "#8ec3f5",
            "functions": "#e9e49d",
            "keywords": "#af828c",
            "numbers": "#830eb1",
            "strings": "#bd764d",
            "types": "#03c0b0",
            "textMateRules": [
                {
                    "scope": "keyword.control",
                    "settings": {
                        "foreground": "#965d6a",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": "keyword.operator",
                    "settings": {
                        "foreground": "#ddd"
                    }
                },
                {
                    "scope": "keyword.other",
                    "settings": {
                        "foreground": "#796b6e",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "entity.name.function.preprocessor",
                    "settings": {
                        "foreground": "#95ad33"
                    }
                },
                {
                    "scope": "entity.name.function.macro",
                    "settings": {
                        "foreground": "#95ad33"
                    }
                },
                {
                    "scope": "storage.type",
                    "settings": {
                        "foreground": "#796b6e"
                    }
                },
                {
                    "scope": "storage.modifier",
                    "settings": {
                        "foreground": "#796b6e"
                    }
                },
                {
                    "scope": "punctuation",
                    "settings": {
                        "foreground": "#ddd",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": "constant",
                    "settings": {
                        "foreground": "#fff"
                    }
                },
                {
                    "scope": "constant.language",
                    "settings": {
                        "foreground": "#4361c5",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "variable.other.constant",
                    "settings": {
                        "foreground": "#fff"
                    }
                },
                {
                    "scope": "variable.other.enummember",
                    "settings": {
                        "foreground": "#1c848b",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "support.type.primitive",
                    "settings": {
                        "foreground": "#007e77"
                    }
                }
                
            ]
        }
    }
}